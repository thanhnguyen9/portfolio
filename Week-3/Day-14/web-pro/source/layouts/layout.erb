<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Web-Pro" %></title>
    
    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body>
    <header class="navigation">
      <div class="navigation-wrapper">
        <a href="javascript:void(0)" class="logo">
          <img src="https://raw.githubusercontent.com/tiy-houston-q4-rails/day-14/master/icons/logo.png" alt="logo.png" alt="Logo Image">
        </a>
        <a href="" class="navigation-menu-button" id="js-mobile-menu">MENU</a>
        <div class="nav">
          <ul id="navigation-menu">
            <li class="nav-link"><a class="header-font" href="javascript:void(0)">Surf & Paddle Co.Blog</a></li>
           
          </ul>
        </div>
        <div class="navigation-tools">
          <div class="search-bar">
            <div class="search-and-submit">
              
              <a href="javascript:void(0)" class="top">About</a>
              <a href="#" class="top">Search</a>
              <img class="top-search" src="https://raw.githubusercontent.com/tiy-houston-q4-rails/day-14/master/icons/search.png" >
            </div>
          </div>
          
        </div>
      </div>
    </header>

    <%= yield %>


    <footer>
      
      <div class="logo">
        
          <img src="https://raw.githubusercontent.com/tiy-houston-q4-rails/day-14/master/icons/theironyard.png">
        <p>The Ironyard Copyright 2013, All Rights Reserved</p>
       
        <div class="last">
          <ul class="aside-stuffs">
              <li class="on-aside"><a class="bottom-link" href="#">Nav Item 1</a></li>
              <li class="on-aside"><a class="bottom-link" href="#">Nav Item 2</a></li>
              <li class="on-aside"><a class="bottom-link" href="#">Nav Item 3</a></li>
              <li class="on-aside"><a class="bottom-link" href="#">Nav Item 5</a></li>
          </ul>
        </div>
      </div>

      
      
    </footer>

  </body>
</html>